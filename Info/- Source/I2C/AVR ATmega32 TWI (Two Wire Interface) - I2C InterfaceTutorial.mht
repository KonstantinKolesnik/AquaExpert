&nbsp;&nbsp;&nbsp; &nbsp;// Wait for acknowledgement<BR>&nbsp;&nbsp; =
&nbsp;recv_data=3DTWDR;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; =
&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; &nbsp;// Get =
