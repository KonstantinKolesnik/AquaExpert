<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:core="using:SmartHub.UWP.Core"
    xmlns:stringResources="using:SmartHub.UWP.Core.StringResources"
    xmlns:vc="using:SmartHub.UWP.Core.Xaml.ValueConverters">
    
	<core:AppManager x:Key="AppManager"/>
    <stringResources:LabelsManager x:Key="LabelsManager"/>

    <vc:StringToUpperStringConverter x:Key="StringToUpperStringConverter"/>
    <vc:StringToBooleanConverter x:Key="StringToBooleanConverter"/>
    <vc:NotNullToVisibilityConverter x:Key="NotNullToVisibilityConverter"/>
    <vc:NotNullToInvisibilityConverter x:Key="NotNullToInvisibilityConverter"/>
    <vc:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
    <vc:BooleanToInvisibilityConverter x:Key="BooleanToInvisibilityConverter"/>
</ResourceDictionary>
